{
	"editor.formatOnSave": true,
	"editor.wordWrapColumn": 80,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"[javascript]": {
		"editor.formatOnSave": true
	},
	"[typescript]": {
		"editor.formatOnSave": true
	},
	"files.exclude": {
		"**/*.map": true
	},
	"prettier.printWidth": 80,
	"prettier.tabWidth": 2,
	"prettier.singleQuote": true,
	"prettier.trailingComma": "all",
	"prettier.bracketSpacing": true
}
